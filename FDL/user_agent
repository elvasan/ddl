CREATE EXTERNAL TABLE IF NOT EXISTS `user_agent`(
  `user_agent_key` varchar(36), 
  `browser_nm` varchar(50), 
  `browser_version_major_nbr` int, 
  `browser_version_minor_nbr` int, 
  `is_mobile_ind` boolean, 
  `os_nm` varchar(50), 
  `os_version_txt` varchar(50), 
  `insert_ts` timestamp, 
  `insert_batch_run_id` int, 
  `insert_job_run_id` int, 
  `source_ts` timestamp, 
  `load_action_ind` varchar(1))
ROW FORMAT SERDE 
  'org.apache.hadoop.hive.ql.io.parquet.serde.ParquetHiveSerDe' 
STORED AS INPUTFORMAT 
  'org.apache.hadoop.mapred.TextInputFormat' 
OUTPUTFORMAT 
  'org.apache.hadoop.hive.ql.io.HiveIgnoreKeyTextOutputFormat'
LOCATION
  's3://jornaya-dev-us-east-1-fdl/user_agent'