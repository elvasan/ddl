CREATE EXTERNAL TABLE IF NOT EXISTS `ip_geolocation`(
  `ip_geolocation_key` varchar(36), 
  `country_cd` varchar(2), 
  `region_cd` varchar(2), 
  `city_nm` varchar(255), 
  `postal_cd` varchar(8), 
  `area_cd` char(3), 
  `isp_nm` varchar(50), 
  `insert_ts` timestamp, 
  `insert_batch_run_id` int, 
  `insert_job_run_id` int, 
  `source_ts` timestamp)
ROW FORMAT SERDE 
  'org.apache.hadoop.hive.ql.io.parquet.serde.ParquetHiveSerDe' 
STORED AS INPUTFORMAT 
  'org.apache.hadoop.mapred.TextInputFormat' 
OUTPUTFORMAT 
  'org.apache.hadoop.hive.ql.io.HiveIgnoreKeyTextOutputFormat'
LOCATION
  's3://jornaya-dev-us-east-1-fdl/ip_geolocation'